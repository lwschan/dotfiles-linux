Host *
    IgnoreUnknown AddKeysToAgent
    AddKeysToAgent yes

Host github.com
  AddKeysToAgent yes
  IdentityFile ~/.ssh/id_ed25519_github

Host homelab1
    User homelab1
    Hostname 10.0.20.50
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/id_ed25519_homelab

